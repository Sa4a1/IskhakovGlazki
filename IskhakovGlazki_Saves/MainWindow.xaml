<Window x:Class="IskhakovGlazki_Saves.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IskhakovGlazki_Saves"
        mc:Ignorable="d"
        Title="Iskhakov_Glazki" Height="450" Width="800" MinWidth="450" MinHeight="800"  FontFamily="Century Gothic"  Icon="/Images/Глазки-save.ico">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Button Name="ListAgentBtn" Content="Показать Список Агентов" FontSize="20" Width="275" Height="45" HorizontalAlignment="Right" VerticalAlignment="Center" Margin="0,0,25,0" Background="#B175EE" Click="ListAgentBtn_Click"/>
        <Image Source="/Images/Глазки-save.png" HorizontalAlignment="Left" Height="200" VerticalAlignment="Center"/>
        <Frame Grid.Row="1" Name="MainFrame"/>
    </Grid>
</Window>
